#ALTER:-
'''
import pickle
lst_master=[]
def MainMenu():
    print("-- MAIN MENU :: OPERATIONS OF BINARY FILES --")
    print("1. Write Data ")
    print("2. Read Data ")
    print("3. Append Data ")
    print("4. Search Data ")
    print("5. Update Data ")
    print("6. Delete Data ")
    print("7. Exit ")
    print("------------------------------------------")

def bin_write():
    f=open(r"A.dat","wb")
    try:
        while True:
            roll=int(input("Enter Roll No:"))
            name=input("Enter name:")
            lst_write=[roll,name]
            lst_master.append(lst_write)
            ch=input("Do you want to continue (y/n)?")
            if ch in 'Nn':
                break
            elif ch not in 'Yy':
                print("Press y to continue, n to exit")
                ch=input("Do you want to continue (y/n)?")
        pickle.dump(lst_master,f)
        print("Data written successfully!")
    except Exception as e:
        f.close()
    f.close()
        
def bin_read():
    f=open(r"A.dat","rb")
    print("Printing data...")
    try:
        data=pickle.load(f)
        print("Roll No","\t","Name")
        for i in data:
            print(i[0],end="\t\t")
            print(i[1])
    except Exception:
        f.close()
    f.close()

def bin_append():
    f=open(r"A.dat","rb+")
    print("Appending data...")
    try:
        roll=int(input("Enter Roll No:"))
        name=input("Enter name:")
        lst_write=[roll,name]
        lst_master.append(lst_write)
        pickle.dump(lst_master,f)
        print("Data appended successfully!")
         
    except Exception:
        f.close()
    f.close()
    
def bin_search():
    f=open(r"A.dat","rb")
    print("Searching data...")
    flag=0
    try:
        data=pickle.load(f)
        srch_rollno = int(input("Enter Roll No to be searched:"))
        for i in data:
            if i[0] == srch_rollno:
                print("Record ", i, " found")
                flag=1
                break
        if flag==0:
            print("No record found")
    except Exception:
        f.close()
    f.close()

def bin_update():
    f=open(r"A.dat","rb+")
    print("Updating data...")
    flag=0
    try:
        data=pickle.load(f)
        srch_rollno = int(input("Enter Roll No for which record is to be updated:"))
        for i in data:
            if i[0] == srch_rollno:
                print("Current name is:",i[1])
                i[1]=input("Enter new name to be updated:")
                flag=1
                break
        if flag==0:
            print("No record found")
        else:
            f.seek(0)
            pickle.dump(data,f)
            print("Data updated successfully!")
    except Exception:
        f.close()
    f.close()

while True:
    MainMenu() 
    ch= int(input("Select the option (1/2/3/4/5/6/7) from the menu:"))
    if ch==1:
        bin_write()
    elif ch==2:
            bin_read()
    elif ch==3:
        bin_append()
    elif ch==4:
        bin_search()
    elif ch==5:
        bin_update()
    elif ch==6:
        bin_delete()
    elif ch==7:
        break
    else:
        print("Select correct option from the menu")
'''
#WAP to count the frequency of each word in a given string.
'''
string=input("Enter a string")
freq = [None] * len(string);  
for i in range(0, len(string)):  
    freq[i] = 1;  
    for j in range(i+1, len(string)):  
        if(string[i] == string[j]):  
            freq[i] = freq[i] + 1;   
            string = string[ : j] + '0' + string[j+1 : ];    
print("Characters and their corresponding frequencies");  
for i in range(0, len(freq)):  
    if(string[i]!=''and string[i]!='0'):  
        print(string[i]+ "-" +str(freq[i])); 
'''
#WAP in python to readlines from a text file notes.txt count and print those lines which are starting with (.) or (,).
'''
f=open(r"C:\\Users\\User.LAB3-PC3\\Desktop\\notes.txt","r")
d=f.readlines()
c=0
for i in d:
    if i.startswith(".") or i.startswith(","):
        c+=1
        print(i)
print(c)
'''
#WAP TO COUNT THE NUMBER OF WORDS IN A TEXT FILE.
'''''
f=open(r"C:\\Users\\User.LAB3-PC3\\Desktop\\notes.txt","r")
s=f.read()
c=0
d=s.split()
for i in d:
    c+=1
    print(i,end=" ")
print("\n",c)
'''
#WRITE A FUNCTION IN PYTHON TO READ CONTENTS FROM A FILE NOTES.TXT TO COUNT AND DISPLAY THE OCCURENCE OF THE WORDS (IS TO UP).
'''
f=open(r"C:\\Users\\User.LAB3-PC3\\Desktop\\notes.txt","r")
s=f.read()
cup=0
cis=0
cto=0
d=s.split()
for i in d:
    if "up"==i:
        cup+=1
    elif "to"==i:
        cto+=1
    elif "is"==i:
        cis+=1
print("Occurence of UP is:",cup)
print("Occurence of TO is:",cto)
print("Occurence of IS is:",cis)
