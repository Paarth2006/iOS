f.write("PYTHON IS FUN AND INTERESTING")
f.close()

f=open(r"myfile.txt","r")
d=f.read()
for i in d:
    f2=open("yourfile.txt","w")
    f2.write(d)
    f2.close()

    f2=open(r"yourfile.txt","r")
    d=f2.read()
    for i in d:
        print(i,end=" ")
    f2.close()
    break
