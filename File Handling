#Menu Driven:-
'''
import pickle
a=int(input("Enter 1 for Write,2 for Read and 3 for Append,4 for Search or 5 for update:"))
lst_master=[]
while True:
    if a==1:
        f=open("qwert.dat","wb")
        ch=input("Enter y or Y to continue:")
        while ch in ("yY"):
            rno = int(input("Enter the rollno:"))
            name=input("Enter the name:")
            lst=[rno,name]
            lst_master.append(lst)
            ch=input("Enter y or Y to continue:")
            if ch not in ("yY"):
                    break
        pickle.dump(lst_master,f)
        f.close()
        print(lst_master)
    a=int(input("Enter 1 for Write,2 for Read and 3 for Append,4 for Search or 5 for Update:"))
    if a==2:
        f=open("qwert.dat","rb")
        d1=pickle.load(f)
        f.close()
        print(d1)
    if a==3:
        f=open("qwert.dat","rb+")
        r1=int(input("Enter roll.no:"))
        n1=input("Enter the Name:")
        lst2=[r1,n1]
        lst_master.append(lst2)
        pickle.dump(lst_master,f)
        f.close()
    if a==4:
        f=open("qwert.dat","rb")
        k2=eval(input(("Enter what to search:")))
        flag=0
        d=pickle.load(f)
        for i in d:
            if i[0]==k2:
                flag=1
        if flag==0:
            print("Record not Found")
        else:
            print("Record Found")
        f.close()
    if a==5:
        f=open("qwert.dat","rb+")
        d2=pickle.load(f)
        r=int(input("Enter what to update:"))
        for i in d:
            if i[0]==r:
                n=input("Enter Updated Name:")
                i[1]=n
                flag=1
        if flag==1:
            f.seek(0)
            pickle.dump(lst,f)
        else:
            print("Record Not Found")
            break
