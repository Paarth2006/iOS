print("Enter 2 to Insert into Table")
print("Enter 3 to Update from Table")
print("Enter 4 to Delete form Table")
print("ENter 5 to see the Table")
n=int(input("Enter your Choice:"))
import mysql.connector
db_connection=mysql.connector.connect(host="localhost",user="root",password="Pass_111",database="employee" )
db_cursor=db_connection.cursor()
while True:
    if n==1:
        def create():
            q=db_cursor.execute("SHOW TABLES")
            if School in q:
                print("Table Already exists")
            else:
                db_cursor.execute("CREATE TABLE School(ID INT PRIMARY KEY, Name VARCHAR(255), Salary INT(6))")
                db_cursor.execute("SHOW TABLES")
                for table in db_cursor:
                        print(table)
                        break
        create()
    if n==2:
        def insert():
            x=int(input("Enter the Number of records you want to enter:"))
            for i in range(x):
                a=int(input("Enter the ID:"))
                b=input("Enter the Name:")
                c=int(input("Enter Salary:"))
                data=("INSERT INTO school(id, name, salary)VALUES({},'{}',{}) where id=1").format(a,b,c)
                db_cursor.execute(data)
                db_connection.commit()
                print(db_cursor.rowcount,"Record Inserted")
        insert()
        break
    if n==3:
        def update():
            updated_ID=int(input("Enter the Updated ID:"))
            updated_Name=input("Enter the Update Name:")
            updated_Salary=int(input("Enter the Update Salary:"))
            data2=("UPDATE SCHOOL set id={}, Name={}, Salary={}").format(updated_ID, updated_Name,updated_Salary)
            db_cursor.execute(data2)
            db_connection.commit()
            print(db_cursor.rowcount,"Record Updated")
        update()
        break
    if n==4:
        def Delete():
            print("deleted")
    if n==5:
        def see():
            db_cursor.execute("SELECT * FROM school")
            d=db_cursor.fetchall()
            for i in d:
                print(i)
        see()
        break
