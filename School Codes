'''
#Write a function that prints prime number between the range(2,n)
'''
def prime(n):
    for i in range(2,n+1):
        flag=True
        n1=n//2
        for j in range(2,n1):
            if n%i==0:
                flag=False
                break
    if flag==True:
        print(n,"Is a prime number")
n=int(input("Enter a Number:"))
prime(n)
'''
#Write a user defined function GenNum(a,b) to generate odd no.s a and b including b.
'''
def GenNum(a,b):
    for i in range(a,b+1):
        if i%2!=0:
            print(i,"Odd Number")
a=int(input("Enter a Number 1:"))
b=int(input("Enter a Number 2:"))
GenNum(a,b)
'''
#WAF that recieves a 4 digit number and calculates the squares of first two digits and last two digits of the number.
'''
def calc(a):
    x=str(a)
    if len(x)==4:
        b=x[:2]
        d=int(b)**2
        c=x[2:]
        e=int(c)**2
        print(d+e)
a=int(input("Enter a 4 numbers:"))
calc(a)
