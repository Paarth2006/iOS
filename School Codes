a=int(input("Enter 1 for PUSH\nEnter 2 for POP\nEnter 3 For Traversal\nEnter 4 for Peek\nEnter 5 for Exit\nEnter Your Choice:"))
stack=[]
while True:
    if a==1:
        def PUSH():
            x=int(input("Enter the Maximum Range:"))
            i=int(input("Enter an Element:"))
            while True:
                if i>x:
                    print("overflow")
                else:
                    stack.append(i)
                    print(stack)
                break
        PUSH()
    a=int(input("Enter 1 for PUSH\nEnter 2 for POP\nEnter 3 For Traversal\nEnter 4 for Peek\nEnter 5 for Exit\nEnter Your Choice:"))
    if a==2:
        def POP():
            while True:
                if stack==[]:
                    print("Underflow")
                else:
                    stack.pop()
                    print(stack)
                break
        POP()
    a=int(input("Enter 1 for PUSH\nEnter 2 for POP\nEnter 3 For Traversal\nEnter 4 for Peek\nEnter 5 for Exit\nEnter Your Choice:"))
    if a==3:
        def TRAVERSAL():
            while True:
                t=stack[::-1]
                print(t)
                break
        TRAVERSAL()
    a=int(input("Enter 1 for PUSH\nEnter 2 for POP\nEnter 3 For Traversal\nEnter 4 for Peek\nEnter 5 for Exit\nEnter Your Choice:"))        
    if a==4:
        def PEEK():
            while True: 
		print(stack[-1])
                	break
        PEEK()
a=int(input("Enter 1 for PUSH\nEnter 2 for POP\nEnter 3 For Traversal\nEnter 4 for Peek\nEnter 5 for Exit\nEnter Your Choice:"))
    if a==5:
        break
